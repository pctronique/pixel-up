{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}


  

    <form id="profilForm" {{ form_start(registrationForm) }}>

    <h1>Inscription</h1>

 

        {{ form_row(registrationForm.username) }}

            <label for="Mot de passe">{{ form_row(registrationForm.plainPassword, {label: 'Mot de passe'}) }}</label>
            
            <span class="iconify" data-icon="icon-park-twotone:game" style="color: white; font-size: 70px;"></span>
        {{ form_row(registrationForm.Conditions) }}

        <div class="flexForm">  
            <button type="submit" class="btn">S'inscrire</button>
        </div>
    
    </form {{ form_end(registrationForm) }}>

{% endblock %}

